{
    "name": "pragmatist/regel",
    "description": "A simple business rules engine.",
    "keywords": ["rule engine", "business rules"],
    "license": "MIT",
    "homepage": "https://github.com/Pragmatist/Regel",
    "authors": [
        {
            "name": "Frank van den Brink",
            "email": "frank@pragmatist.nl",
            "homepage": "https://pragmatist.nl"
        }
    ],
    "require": {
        "php":  ">=5.5"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.4",
        "mockery/mockery": "0.9.*",
        "squizlabs/php_codesniffer": "~2.2"
    },
    "autoload": {
        "psr-4": {
            "Pragmatist\\Regel\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Pragmatist\\Regel\\Tests\\": "tests"
        }
    },
    "config": {
        "bin-dir": "bin"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.0.x-dev"
        }
    }
}
